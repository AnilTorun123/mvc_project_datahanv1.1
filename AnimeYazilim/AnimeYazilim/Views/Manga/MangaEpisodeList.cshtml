@using EntityLayer.Concrete
@model List<MangaEpisode>
@{
    ViewBag.Title = "MangaEpisodeList";
    Layout = "~/Views/Shared/_AnimeLayout.cshtml";
}

<div class="section-title">
    <h5>Bölümler</h5>
</div>

@foreach (var item in Model)
{
    <div class="row">
        <div class="col-lg-12">
            <div class="anime__details__episodes">
                <a href="@item.MangaEpPage1">@item.MangaEpNum - @item.Manga.MangaName</a>
            </div>

        </div>
    </div>
}




    @*//burada href ile link vermek yerine bir yönlendirme yapacagım slayt olacak sekilde vs sonra düsünürüm o kısmı tıklayınca tüm pageleri acılacak*@
