
@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

<script src="~/Scripts/jquery-3.4.1.js"></script>

<button class="btn btn-primary" id="btn1">btn1</button>
<button class="btn btn-warning" id="btn2">btn2</button>

<script>
    $('#btn1').click(function () {
        swal("deneme");
    });
</script>

<script>
    $('#btn2').click(function () {
        swal("Sayfanın Amacı", "Çeşitli yapılar ve teknolojileri deniyeceğim belki bir alanda kullanabileceğim hoş yapıları tutacağım alan (sweet alert gibi..)");
    });
</script>

